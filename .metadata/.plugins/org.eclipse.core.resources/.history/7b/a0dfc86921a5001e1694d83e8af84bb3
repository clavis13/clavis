package 산술연산자;

public class EX01산술연산자 {
public static void main(String[] args) {
	
		// 산술연산자 ->  /: 몫   %:나머지
	
		int num1 = 10;
		float num2 = 7f;
		
		System.out.println(num1/num2);
		// 정수와 실수를 연산하게 되면 자동으로 실수로 변환 (실수의 기본형이 double 8byte이기때문에)
		
		// +기호는 더하기뿐만이 아니라 연결 연산을 뜻하기도 함
		int num3 = 10;
		String num4 = "7";
		System.out.println(num3+num4);
		
		
	
	
	
	
	
}
}
